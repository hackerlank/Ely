<?xml version="1.0"?>
<Game>
	<!-- Object Templates Definition -->
	<ObjectTmplSet>
		<ObjectTmpl type="Actor">
			<ComponentTmpl family="Graphics" type="Model"/>
			<ComponentTmpl family="Input" type="ControlByEvent"/>
			<ComponentTmpl family="Audio" type="Sound3d"/>
		</ObjectTmpl>
		<ObjectTmpl type="InstancedActor">
			<ComponentTmpl family="Graphics" type="InstanceOf"/>
		</ObjectTmpl>
		<ObjectTmpl type="StaticActor">
			<ComponentTmpl family="Graphics" type="Model"/>
		</ObjectTmpl>
	</ObjectTmplSet>
	<!-- Objects and Scene Creation -->
	<ObjectSet>
		<Object type="InstancedActor" id="InstancedActor1" priority="90">
			<Param parent="Render"/>
			<Param is_static="true"/>
			<Param pos_x="-10.0"/>
			<Param pos_y="0.0"/>
			<Param pos_z="0.0"/>
			<Param rot_h="-90.0"/>
			<Param rot_p="0.0"/>
			<Param rot_r="0.0"/>
			<Param scale_x="0.5"/>
			<Param scale_y="0.5"/>
			<Param scale_z="1.0"/>
<!--		
			<Parent>Render</Parent>
			<InstanceOf>Actor1</InstanceOf>
			<Position x="-10.0" y="0.0" z="0.0"/>
			<Orientation h="-90.0" p="0.0" r="0.0"/>
			<Scaling x="0.5" y="0.5" z="1.0"/>
-->
			<Component type="InstanceOf">
				<Param instance_of="Actor1"/>
			</Component>
		</Object>
		<Object type="StaticActor" id="Teapot1">
			<Component type="Model">
				<Param model_file="teapot"/>
				<Param is_static="true"/>
			</Component>
			<Parent>Actor1</Parent>
			<InstanceOf></InstanceOf>
			<Position x="0.0" y="-10.0" z="0.0"/>
			<Orientation h="-90.0" p="0.0" r="0.0"/>
			<Scaling x="0.5" y="0.5" z="0.5"/>	
		</Object>
		<Object type="Actor" id="Actor1" priority="100">
			<Component type="ControlByEvent" priority="90">
				<Param enabled="false"/>
				<Param speed="200.0"/>
				<Param fast_factor="10.0"/>
				<Param inverted="false"/>
				<Param up=""/>
				<Param down=""/>
				<Param mouse_enabled_h="true"/>
				<Param mouse_enabled_p="false"/>
			</Component>
			<Component type="Model" priority="100">
				<Param model_file="panda"/>
				<Param anim_file="panda-walk"/>
			</Component>
			<Component type="Sound3d" priority="90">
				<Param sound_file="/usr/share/panda3d/models/audio/sfx/GUI_rollover.wav"/>
			</Component>
			<Parent>Render</Parent>
			<InstanceOf></InstanceOf>
			<Position x="0.0" y="0.0" z="0.0"/>
			<Orientation h="-90.0" p="0.0" r="0.0"/>
			<Scaling x="1.0" y="1.0" z="1.0"/>
		</Object>
	</ObjectSet>
</Game>
