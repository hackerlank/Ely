<?xml version="1.0"?>
<Game>
	<!-- ************************************************ -->
	<!-- ************************************************ -->
	<!-- 		Object Templates Definitions              -->
	<!-- ************************************************ -->
	<!-- ************************************************ -->
	<ObjectTmplSet>
		<!-- template for wrapper around predefined nodepath - Mandatory -->
		<ObjectTmpl type="Render">
			<ComponentTmpl family="Scene" type="NodePathWrapper" priority="100"/>
		</ObjectTmpl>

		<!-- template used only to perform additional initializations/finalizations
			 besides the ones performed by GameManager -->
		<ObjectTmpl type="Game">
			<ComponentTmpl family="Common" type="GameConfig" priority="100">
				<Param event_types="move_car:steady_car"/>
				<Param event_types="onground_character:inair_character"/>
				<Param event_types="path_following:avoid_obstacle"/>
				<Param event_types="avoid_close_neighbor:avoid_neighbor"/>
				<Param event_types="overlap:overlap_off"/>
				<Param event_types="car_character_collision:car_character_collision_off"/>
				<Param event_types="actor_car_collision:actor_car_collision_off"/>
			</ComponentTmpl>
		</ObjectTmpl>
		
		<!-- template for camera object(s) -->
		<ObjectTmpl type="CameraDriver">
			<ComponentTmpl family="Control" type="Driver">
				<Param event_types="forward:stop-forward:fast-forward"/>
				<Param event_types="backward:stop-backward:fast-backward"/>
				<Param event_types="strafe_left:stop-strafe_left:fast-strafe_left"/>
				<Param event_types="strafe_right:stop-strafe_right:fast-strafe_right"/>
				<Param event_types="head_left:stop-head_left:fast-head_left"/>
				<Param event_types="head_right:stop-head_right:fast-head_right"/>
				<Param event_types="pitch_up:stop-pitch_up:fast-pitch_up"/>
				<Param event_types="pitch_down:stop-pitch_down:fast-pitch_down"/>
				<Param event_types="up:stop-up:fast-up"/>
				<Param event_types="down:stop-down:fast-down"/>				
				<Param event_types="fast:stop-fast"/>				
			</ComponentTmpl>
		</ObjectTmpl>
		<ObjectTmpl type="CameraChaser">
			<ComponentTmpl family="Control" type="Chaser">
				<Param event_types="head_left:stop-head_left"/>
				<Param event_types="head_right:stop-head_right"/>
				<Param event_types="pitch_up:stop-pitch_up"/>
				<Param event_types="pitch_down:stop-pitch_down"/>
				<Param event_types="hold_lookat:stop-hold_lookat"/>
			</ComponentTmpl>
		</ObjectTmpl>
		<ObjectTmpl type="Camera">
			<ComponentTmpl family="Scene" type="NodePathWrapper" priority="100"/>
			<ComponentTmpl family="Audio" type="Listener"/>
		</ObjectTmpl>	
		
		<ObjectTmpl type="Actor">
<!--
			<ComponentTmpl family="Behavior" type="Activity">
				<Param states="forward:backward"/>
				<Param states="strafe_left:strafe_right"/>
				<Param states="up:down"/>
				<Param from_to="forward@strafe_left"/>
				<Param from_to="forward@strafe_right"/>
				<Param event_types="forward:stop-forward:fast-forward"/>
				<Param event_types="backward:stop-backward:fast-backward"/>
				<Param event_types="head_left:stop-head_left:fast-head_left"/>
				<Param event_types="head_right:stop-head_right:fast-head_right"/>
				<Param event_types="up:stop-up:fast-up"/>
				<Param event_types="down:stop-down:fast-down"/>
				<Param event_types="fast:stop-fast"/>							
			</ComponentTmpl>
			<ComponentTmpl family="Control" type="Driver"/>
--> 
			<ComponentTmpl family="Scene" type="Model" priority="100"/>
			<ComponentTmpl family="Audio" type="Sound3d"/>
			<ComponentTmpl family="Physics" type="RigidBody" priority="90"/>
		</ObjectTmpl>

		<ObjectTmpl type="HUD">
			<ComponentTmpl family="Behavior" type="Activity">
				<Param states="idle:fire"/>
				<Param states="walk:run"/>
				<Param from_to="walk@run:fire@walk"/>
				<Param from_to="idle@run:fire@idle"/>
				<Param event_types="fire:stop-fire"/>
			</ComponentTmpl>
			<ComponentTmpl family="Audio" type="Sound3d"/>
			<ComponentTmpl family="Scene" type="Model" priority="100"/>
		</ObjectTmpl>

		<ObjectTmpl type="Character">
			<ComponentTmpl family="Behavior" type="Activity">
				<Param states="I:F:B:Sr:Sl:Rr:Rl:J"/>
				<Param states="F-Rr:F-Rl:F-Rr-Rl:Rr-Rl:F-J:B-Rr:B-Rl"/>
				<Param states="Sr-Rr:Sr-Rl:Sl-Rr:Sl-Rl"/>
				<Param states="F-Q:Sr-Q:Sl-Q:Rr-Q:Rl-Q"/>
				<Param states="F-Rr-Q:F-Rl-Q:F-Rr-Rl-Q:Rr-Rl-Q:F-J-Q"/>
				<Param states="Sr-Rr-Q:Sr-Rl-Q:Sl-Rr-Q:Sl-Rl-Q"/>
				<Param event_types="f:f-up:f-q:b:b-up:b-q"/>
				<Param event_types="sr:sr-up:sr-q:sl:sl-up:sl-q"/>
				<Param event_types="rr:rr-up:rr-q:rl:rl-up:rl-q"/>
				<Param event_types="j:j-up:j-q"/>				
				<Param event_types="q:q-up"/>				
				<Param event_types="ground:air"/>
				<Param transition_table="I,f@F:I,f-q@F-Q:I,b@B:I,sr@Sr:I,sr-q@Sr-Q:I,sl@Sl:I,sl-q@Sl-Q:I,rr@Rr:I,rr-q@Rr-Q:I,rl@Rl:I,rl-q@Rl-Q:I,j@J"/>
				<Param transition_table="F,f-up@I:F,f-q@F-Q:F,sr@F-Sr:F,sl@F-Sl:F,rr@F-Rr:F,rr-q@F-Rr-Q:F,rl@F-Rl:F,rl-q@F-Rl-Q:F,j@F-J:F,j-q@F-J-Q:F,q@F-Q"/>
				<Param transition_table="B,b-up@I:B,sr@B-Sr:B,sl@B-Sl:B,rr@B-Rr:B,rl@B-Rl"/>
				<Param transition_table="Sr,sr-up@I:Sr,sr-q@Sr-Q:Sr,rr@Sr-Rr:Sr,rr-q@Sr-Rr-Q:Sr,rl@Sr-Rl:Sr,rl-q@Sr-Rl-Q:Sr,q@Sr-Q:Sl,sl-up@I"/>
				<Param transition_table="Sl,sl-q@Sl-Q:Sl,rr@Sl-Rr:Sl,rr-q@Sl-Rr-Q:Sl,rl@Sl-Rl:Sl,rl-q@Sl-Rl-Q:Sl,q@Sl-Q"/>
				<Param transition_table="Rr,f@F-Rr:Rr,f-q@F-Rr-Q:Rr,b@B-Rr:Rr,sr@Sr-Rr:Rr,sr-q@Sr-Rr-Q:Rr,sl@Sl-Rr:Rr,sl-q@Sr-Rl-Q:Rr,rr-up@I:Rr,rr-q@Rr-Q:Rr,q@Rr-Q"/>
				<Param transition_table="Rl,f@F-Rl:Rl,f-q@F-Rl-Q:Rl,b@B-Rl:Rl,sr@Sr-Rl:Rl,sr-q@Sr-Rl-Q:Rl,sl@Sl-Rl:Rl,sl-q@Sl-Rl-Q:Rl,rl-up@I:Rl,rl-q@Rl-Q:Rl,q@Rl-Q"/>
				<Param transition_table="J,f@F-J:J,f-q@F-J-Q:J,j-up@I"/>
				<Param transition_table="F-Rr,f-up@Rr:F-Rr,f-q@F-Rr-Q:F-Rr,rr-up@F:F-Rr,rr-q@F-Rr-Q:F-Rr,rl@F-Rr-Rl:F-Rr,rl-q@F-Rr-Rl-Q:F-Rr,q@F-Rr-Q:F-Rl,f-up@Rl:F-Rl,f-q@F-Rl-Q:F-Rl,rr@F-Rr-Rl:F-Rl,rr-q@F-Rr-Rl-Q:F-Rl,rl-up@F:F-Rl,rl-q@F-Rl-Q:F-Rl,q@F-Rl-Q:F-Rr-Rl,f-up@Rr-Rl:F-Rr-Rl,f-q@F-Rr-Rl-Q:F-Rr-Rl,rr-up@F-Rl:F-Rr-Rl,rr-q@F-Rr-Rl-Q:F-Rr-Rl,rl-up@F-Rr:F-Rr-Rl,rl-q@F-Rr-Rl-Q:F-Rr-Rl,q@F-Rr-Rl-Q"/>
				<Param transition_table="Rr-Rl,f@F-Rr-Rl:Rr-Rl,f-q@F-Rr-Rl-Q:Rr-Rl,rr-up@Rl:Rr-Rl,rr-q@Rr-Rl-Q:Rr-Rl,rl-up@Rr:Rr-Rl,rl-q@Rr-Rl-Q:Rr-Rl,q@Rr-Rl-Q"/>
				<Param transition_table="F-J,f-up@J:F-J,f-q@F-J-Q:F-J,j-up@F:F-J,j-q@F-J-Q:F-J,q@F-J-Q"/>
				<Param transition_table="B-Rr,b-up@Rr:B-Rr,rr-up@B:B-Rl,b-up@Rl:B-Rl,rl-up@B"/>
				<Param transition_table="Sr-Rr,sr-up@Rr:Sr-Rr,sr-q@Sr-Rr-Q:Sr-Rr,rr-up@Sr:Sr-Rr,rr-q@Sr-Rr-Q:Sr-Rr,q@Sr-Rr-Q:Sr-Rl,sr-up@Rl:Sr-Rl,sr-q@Sr-Rl-Q:Sr-Rl,rl-up@Sr:Sr-Rl,rl-q@Sr-Rl-Q:Sr-Rl,q@Sr-Rl-Q"/>
				<Param transition_table="Sl-Rr,sl-up@Rr:Sl-Rr,sl-q@Sl-Rr-Q:Sl-Rr,rr-up@Sl:Sl-Rr,rr-q@Sl-Rr-Q:Sl-Rr,q@Sl-Rr-Q:Sl-Rl,sl-up@Rl:Sl-Rl,sl-q@Sl-Rl-Q:Sl-Rl,rl-up@Sl:Sl-Rl,rl-q@Sl-Rl-Q:Sl-Rl,q@Sl-Rl-Q:F-Q,f-up@I"/>
				<Param transition_table="F-Q,rr@F-Rr-Q:F-Q,rr-q@F-Rr-Q:F-Q,rl@F-Rl-Q:F-Q,rl-q@F-Rl-Q:F-Q,j@F-J-Q:F-Q,q-up@F"/>
				<Param transition_table="Sr-Q,sr-up@I:Sr-Q,rr@Sr-Rr-Q:Sr-Q,rr-q@Sr-Rr-Q:Sr-Q,rl@Sr-Rl-Q:Sr-Q,rl-q@Sr-Rl-Q:Sr-Q,q-up@Sr"/>
				<Param transition_table="Sl-Q,sl-up@I:Sl-Q,rr@Sl-Rr-Q:Sl-Q,rr-q@Sl-Rr-Q:Sl-Q,rl@Sl-Rl-Q:Sl-Q,rl-q@Sl-Rl-Q:Sl-Q,q-up@Sl"/>
				<Param transition_table="Rr-Q,f@F-Rr-Q:Rr-Q,f-q@F-Rr-Q:Rr-Q,sr@Sr-Rr-Q:Rr-Q,sr-q@Sr-Rr-Q:Rr-Q,sl@Sl-Rr-Q:Rr-Q,sl-q@Sl-Rr-Q:Rr-Q,rr-up@I:Rr-Q,q-up@Rr"/>
				<Param transition_table="Rl-Q,f@F-Rl-Q:Rl-Q,f-q@F-Rl-Q:Rl-Q,sr@Sr-Rl-Q:Rl-Q,sr-q@Sr-Rl-Q:Rl-Q,sl@Sl-Rl-Q:Rl-Q,sl-q@Sl-Rl-Q:Rl-Q,rl-up@I:Rl-Q,q-up@Rl"/>
				<Param transition_table="F-Rr-Q,f-up@Rr-Q:F-Rr-Q,rr-up@F-Q:F-Rr-Q,rl@F-Rr-Rl:F-Rr-Q,rl-q@F-Rr-Rl-Q:F-Rr-Q,q-up@F-Rr:F-Rl-Q,f-up@Rl-Q:F-Rl-Q,rr@F-Rr-Rl:F-Rl-Q,rr-q@F-Rr-Rl-Q:F-Rl-Q,rl-up@F-Q:F-Rl-Q,q-up@F-Rl:F-Rr-Rl-Q,f-up@Rr-Rl-Q:F-Rr-Rl-Q,rr-up@F-Rl-Q:F-Rr-Rl-Q,rl-up@F-Rr-Q:F-Rr-Rl-Q,q-up@F-Rr-Rl"/>
				<Param transition_table="Rr-Rl-Q,f@F-Rr-Rl-Q:Rr-Rl-Q,f-q@F-Rr-Rl-Q:Rr-Rl-Q,rr-up@Rl-Q:Rr-Rl-Q,rl-up@Rr-Q:Rr-Rl-Q,q-up@Rr-Rl"/>
				<Param transition_table="F-J-Q,f-up@J:F-J-Q,j-up@F-Q:F-J-Q,q-up@F-J"/>
				<Param transition_table="Sr-Rr-Q,sr-up@Rr-Q:Sr-Rr-Q,rr-up@Sr-Q:Sr-Rr-Q,q-up@Sr-Rr:Sr-Rl-Q,sr-up@Rl-Q:Sr-Rl-Q,rl-up@Sr-Q:Sr-Rl-Q,q-up@Sr-Rl"/>
				<Param transition_table="Sl-Rr-Q,sl-up@Rr-Q:Sl-Rr-Q,rr-up@Sl-Q:Sl-Rr-Q,q-up@Sl-Rr:Sl-Rl-Q,sl-up@Rl-Q:Sl-Rl-Q,rl-up@Sl-Q:Sl-Rl-Q,q-up@Sr-Rl"/>
			</ComponentTmpl>
			<ComponentTmpl family="AI" type="SteerVehicle" priority="80"/>
			<ComponentTmpl family="Scene" type="Model" priority="100"/>
			<ComponentTmpl family="Audio" type="Sound3d"/>
			<ComponentTmpl family="PhysicsControl" type="CharacterController" priority="90"/> 
		</ObjectTmpl>

		<!-- templates for AI Recast Navigation -->
		<ObjectTmpl type="RecastCrowdAgent">
			<ComponentTmpl family="Scene" type="InstanceOf" priority="100"/>
			<ComponentTmpl family="AI" type="CrowdAgent"/>
			<ComponentTmpl family="Audio" type="Sound3d"/>
			<ComponentTmpl family="Physics" type="RigidBody" priority="90"/>
		</ObjectTmpl>

		<ObjectTmpl type="RecastNavMesh">
			<ComponentTmpl family="Scene" type="Model" priority="100"/>
			<ComponentTmpl family="Physics" type="RigidBody" priority="90"/> 
			<ComponentTmpl family="AI" type="NavMesh">
				<Param event_types="add_crowd_agent:remove_crowd_agent"/>
				<Param event_types="toggle_debug"/>
			</ComponentTmpl>
		</ObjectTmpl>

		<!-- templates for AI OpenSteer -->
		<ObjectTmpl type="OpenSteerVehicle">
			<ComponentTmpl family="Scene" type="InstanceOf" priority="100"/>
			<ComponentTmpl family="AI" type="SteerVehicle" priority="80"/>
			<ComponentTmpl family="Audio" type="Sound3d"/>
			<ComponentTmpl family="Physics" type="RigidBody" priority="90"/>
		</ObjectTmpl>

		<ObjectTmpl type="OpenSteerPlugIn">
			<ComponentTmpl family="AI" type="SteerPlugIn">
				<Param event_types="toggle_debug"/>
			</ComponentTmpl>
		</ObjectTmpl>

		<!-- templates for vehicles -->
		<ObjectTmpl type="Car">
			<ComponentTmpl family="Scene" type="InstanceOf" priority="100"/>
			<ComponentTmpl family="Physics" type="RigidBody" priority="90"/>
			<ComponentTmpl family="PhysicsControl" type="Vehicle" priority="80">
				<Param event_types="forward:stop-forward"/>
				<Param event_types="backward:stop-backward"/>
				<Param event_types="turn_left:stop-turn_left"/>
				<Param event_types="turn_right:stop-turn_right"/>
				<Param event_types="brake:stop-brake"/>
			</ComponentTmpl>
		</ObjectTmpl>
		
		<ObjectTmpl type="CarWheel">
			<ComponentTmpl family="Scene" type="InstanceOf" priority="100"/>
		</ObjectTmpl>

		<!-- templates for soft bodies -->
		<ObjectTmpl type="SoftObject">
			<ComponentTmpl family="Scene" type="Model" priority="100"/>
			<ComponentTmpl family="Physics" type="SoftBody" priority="90"/> 
		</ObjectTmpl>
		
		<!-- templates for static objects -->		
		<ObjectTmpl type="StaticActor">
			<ComponentTmpl family="Scene" type="Model" priority="100"/>
			<ComponentTmpl family="Physics" type="RigidBody" priority="90"/> 
		</ObjectTmpl>
		
		<ObjectTmpl type="Terrain">
			<ComponentTmpl family="Scene" type="Terrain" priority="100"/>
			<ComponentTmpl family="Physics" type="RigidBody" priority="90"/> 
		</ObjectTmpl>

		<ObjectTmpl type="GhostObject">
			<ComponentTmpl family="Scene" type="InstanceOf" priority="100"/>
			<ComponentTmpl family="Physics" type="Ghost" priority="90"/> 
		</ObjectTmpl>

		<!-- templates for instancing -->
		<ObjectTmpl type="Instanceable">
			<ComponentTmpl family="Scene" type="Model" priority="100"/>
		</ObjectTmpl>
		
		<ObjectTmpl type="InstancedActor">
			<ComponentTmpl family="Scene" type="InstanceOf" priority="100"/>
			<ComponentTmpl family="Audio" type="Sound3d"/>
			<ComponentTmpl family="Physics" type="RigidBody" priority="90"/> 
		</ObjectTmpl>

	</ObjectTmplSet>

	<!-- ************************************************ -->
	<!-- ************************************************ -->
	<!-- 		Objects and Scene Creation                -->
	<!-- ************************************************ -->
	<!-- ************************************************ -->	
	<ObjectSet>

		<!-- ************** -->
		<!-- CAMERA OBJECTS -->
		<!-- ************** -->
	
		<Object type="CameraDriver" id="cameraDriverTmp" priority="300">
			<Param store_params="true"/>
			<Component type="Driver">
				<Param enabled="false"/>
				<Param max_linear_speed="40.0"/>
				<Param max_angular_speed="40.0"/>
				<Param linear_accel="30.0"/>
				<Param angular_accel="30.0"/>
				<Param linear_friction="5.0"/>
				<Param angular_friction="5.0"/>
				<Param stop_threshold="0.0"/>
				<Param fast_factor="5.0"/>
				<Param sens_x="0.05"/>
				<Param sens_y="0.05"/>
				<Param pitch_limit="true@60.0"/>
				<Param head_left="enabled"/>
				<Param head_right="enabled"/>
				<Param inverted_translation="true"/>
				<Param inverted_rotation="false"/>
				<Param mouse_enabled_h="true"/>
				<Param mouse_enabled_p="true"/>
				<Param events="forward@mouse3:stop-forward@mouse3-up:fast-forward@shift-mouse3$"/>
				<Param events="backward@x:stop-backward@x-up:fast-backward@shift-x$"/>
				<Param events="strafe_left@q:stop-strafe_left@q-up:fast-strafe_left@shift-q$"/>
				<Param events="strafe_right@e:stop-strafe_right@e-up:fast-strafe_right@shift-e$"/>
				<Param events="head_left@a:stop-head_left@a-up:fast-head_left@shift-a$"/>
				<Param events="head_right@d:stop-head_right@d-up:fast-head_right@shift-d$"/>
				<Param events="pitch_up@w:stop-pitch_up@w-up:fast-pitch_up@shift-w$"/>
				<Param events="pitch_down@s:stop-pitch_down@s-up:fast-pitch_down@shift-s$"/>
				<Param events="up@r:stop-up@r-up:fast-up@shift-r$"/>
				<Param events="down@f:stop-down@f-up:fast-down@shift-f$"/>				
				<Param events="fast@shift:stop-fast@shift-up$"/>
			</Component>
		</Object>
		
		<Object type="CameraChaser" id="cameraChaserTmp" priority="300">
			<Param store_params="true"/>
			<Component type="Chaser">
				<Param enabled="false"/>
				<Param backward="false"/>
				<Param chased_object="NONE"/>
				<Param fixed_relative_position="false"/>
				<Param reference_object="render"/>
				<Param abs_max_distance="25.0"/>
				<Param abs_min_distance="18.0"/>
				<Param abs_max_height="8.0"/>
				<Param abs_min_height="5.0"/>
				<Param friction="5.0"/>
				<Param fixed_lookat="false"/>
				<Param mouse_enabled_h="true"/>
				<Param mouse_enabled_p="true"/>
				<Param abs_lookat_distance="5.0"/>
				<Param abs_lookat_height="1.5"/>
				<Param events="head_left@a:stop-head_left@a-up$"/>
				<Param events="head_right@d:stop-head_right@d-up$"/>
				<Param events="pitch_up@w:stop-pitch_up@w-up$"/>
				<Param events="pitch_down@s:stop-pitch_down@s-up$"/>
				<Param events="hold_lookat@shift:stop-hold_lookat@shift-up$"/>
			</Component>
		</Object>

		<Object type="Camera" id="camera" priority="200">
			<Component type="NodePathWrapper">
				<Param nodepath="camera"/>
			</Component>
			<Param parent=""/>
			<Param is_steady="false"/>		
			<Param pos="-35.0,-94.0,96.0"/>
			<Param rot="0.0,-28.0,0.0"/>
		</Object>

		<!-- ************* -->
		<!-- WORLD OBJECTS -->
		<!-- ************* -->

		<Object type="Instanceable" id="Smiley1" priority="120">
			<Component type="Model">
				<Param model_file="smiley.bam"/>
			</Component>
			<Param parent=""/>
			<Param is_steady="true"/>
		</Object>
		<Object type="Instanceable" id="Panda1" priority="120">
			<Component type="Model">
				<Param model_file="panda.bam"/>
			</Component>
			<Param parent=""/>
			<Param is_steady="true"/>
		</Object>
		<Object type="Instanceable" id="Gorilla1" priority="120">
			<Component type="Model">
				<Param model_file="walk@gorillawalking.bam"/>
			</Component>
			<Param parent=""/>
			<Param is_steady="true"/>
		</Object>

		<Object type="InstancedActor" id="ball1">
			<Param parent="render"/>
			<Param is_steady="false"/>
			<Param pos="-9.60363,-21.9574,70.9806"/>
			<Param rot="180.0,0.0,0.0"/>
			<Component type="RigidBody">
				<Param body_type="dynamic"/>
				<Param body_mass="4.0"/>
				<Param body_friction="0.8"/>
				<Param body_restitution="0.5"/>
				<Param shape_type="sphere"/>
				<Param shape_size="minimum"/>
				<Param collide_mask="all_on"/>
			</Component> 
			<Component type="InstanceOf">
				<Param instance_of="Smiley1"/>
				<Param scale="2.0,2.0,2.0"/>
			</Component>
		</Object>
		
		<Object type="Actor" id="Actor1"  priority="100">
			<Component type="Activity">
				<Param events="forward@h:stop-forward@h-up:fast-forward@shift-h$"/>
				<Param events="backward@b:stop-backward@b-up:fast-backward@shift-b$"/>
				<Param events="head_left@v:stop-head_left@v-up:fast-head_left@shift-v$"/>
				<Param events="head_right@n:stop-head_right@n-up:fast-head_right@shift-n$"/>
				<Param events="up@page_up:stop-up@page_up-up:fast-up@shift-page_up$"/>
				<Param events="down@page_down:stop-down@page_down-up:fast-down@shift-page_down$"/>
				<Param events="fast@shift:stop-fast@shift-up$"/>				
			</Component>
			<Component type="Driver">
				<Param enabled="false"/>
				<Param max_linear_speed="15.0"/>
				<Param max_angular_speed="30.0"/>
				<Param fast_factor="15.0"/>
				<Param inverted_translation="false"/>
				<Param inverted_rotation="false"/>
				<Param mouse_enabled_h="true"/>
				<Param mouse_enabled_p="false"/>
			</Component>
			<Component type="Model">
				<Param model_file="panda.bam"/>
				<Param anim_files="walk@panda-walk.bam"/>
				<Param scale="0.6,0.6,0.6"/>
			</Component>
			<Component type="RigidBody">
				<Param body_type="dynamic"/>
				<Param body_mass="1.0"/>
				<Param body_friction="0.8"/>
				<Param body_restitution="0.1"/>
				<Param shape_type="capsule"/>
				<Param shape_size="maximum"/>
				<Param collide_mask="all_on"/>
