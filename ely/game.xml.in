<?xml version="1.0"?>
<Game>
	<!-- ------------------------------------------------ -->
	<!-- ------------------------------------------------ -->
	<!-- 		Object Templates Definitions              -->
	<!-- ------------------------------------------------ -->
	<!-- ------------------------------------------------ -->
	<ObjectTmplSet>
		<!-- template for wrapper around predefined nodepath - Mandatory -->
		<ObjectTmpl type="Render">
			<ComponentTmpl family="Scene" type="NodePathWrapper" priority="200"/>
		</ObjectTmpl>

		<!-- template for a singleton game object: used only to perform additional
			 custom initializations besides the ones performed by GameManager -->
		<ObjectTmpl type="Game"/>
		
		<!-- template for camera object(s) -->
		<ObjectTmpl type="Camera">
			<ComponentTmpl family="Scene" type="NodePathWrapper" priority="200"/>
<!--
			<ComponentTmpl family="Control" type="Chaser" priority="60">
				<Param event_types="head_left:stop-head_left"/>
				<Param event_types="head_right:stop-head_right"/>
				<Param event_types="pitch_up:stop-pitch_up"/>
				<Param event_types="pitch_down:stop-pitch_down"/>
				<Param event_types="hold_lookat:stop-hold_lookat"/>
			</ComponentTmpl>
-->
			<ComponentTmpl family="Control" type="Driver" priority="60">
				<Param event_types="forward:stop-forward:fast-forward"/>
				<Param event_types="backward:stop-backward:fast-backward"/>
				<Param event_types="strafe_left:stop-strafe_left:fast-strafe_left"/>
				<Param event_types="strafe_right:stop-strafe_right:fast-strafe_right"/>
				<Param event_types="roll_left:stop-roll_left:fast-roll_left"/>
				<Param event_types="roll_right:stop-roll_right:fast-roll_right"/>
				<Param event_types="up:stop-up:fast-up"/>
				<Param event_types="down:stop-down:fast-down"/>				
				<Param event_types="fast:stop-fast"/>				
			</ComponentTmpl>
			<ComponentTmpl family="Audio" type="Listener" priority="50"/>
		</ObjectTmpl>
		
		<ObjectTmpl type="Actor">
<!--
			<ComponentTmpl family="Behavior" type="Activity" priority="40">
				<Param states="forward:backward"/>
				<Param states="strafe_left:strafe_right"/>
				<Param states="up:down"/>
				<Param from_to="forward@strafe_left"/>
				<Param from_to="forward@strafe_right"/>
				<Param event_types="forward:stop-forward:fast-forward"/>
				<Param event_types="backward:stop-backward:fast-backward"/>
				<Param event_types="roll_left:stop-roll_left:fast-roll_left"/>
				<Param event_types="roll_right:stop-roll_right:fast-roll_right"/>
				<Param event_types="up:stop-up:fast-up"/>
				<Param event_types="down:stop-down:fast-down"/>
				<Param event_types="fast:stop-fast"/>							
			</ComponentTmpl>
			<ComponentTmpl family="Control" type="Driver" priority="60"/>
--> 
			<ComponentTmpl family="Scene" type="Model" priority="100"/>
			<ComponentTmpl family="Audio" type="Sound3d" priority="50"/>
			<ComponentTmpl family="Physics" type="RigidBody" priority="90"/>
		</ObjectTmpl>

		<ObjectTmpl type="HUD">
			<ComponentTmpl family="Behavior" type="Activity" priority="40">
				<Param states="idle:fire"/>
				<Param states="walk:run"/>
				<Param from_to="walk@run:fire@walk"/>
				<Param from_to="idle@run:fire@idle"/>
				<Param event_types="fire:stop-fire"/>
			</ComponentTmpl>
			<ComponentTmpl family="Audio" type="Sound3d" priority="50"/>
			<ComponentTmpl family="Scene" type="Model" priority="100"/>
		</ObjectTmpl>

		<ObjectTmpl type="Character">
			<ComponentTmpl family="Behavior" type="Activity" priority="40">
				<Param states="I:F:B:Sr:Sl:Rr:Rl:J"/>
				<Param states="F-Rr:F-Rl:F-Rr-Rl:Rr-Rl:F-J:B-Rr:B-Rl"/>
				<Param states="Sr-Rr:Sr-Rl:Sl-Rr:Sl-Rl"/>
				<Param states="F-Q:Sr-Q:Sl-Q:Rr-Q:Rl-Q"/>
				<Param states="F-Rr-Q:F-Rl-Q:F-Rr-Rl-Q:Rr-Rl-Q:F-J-Q"/>
				<Param states="Sr-Rr-Q:Sr-Rl-Q:Sl-Rr-Q:Sl-Rl-Q"/>
				<Param event_types="f:f-up:f-q:b:b-up:b-q"/>
				<Param event_types="sr:sr-up:sr-q:sl:sl-up:sl-q"/>
				<Param event_types="rr:rr-up:rr-q:rl:rl-up:rl-q"/>
				<Param event_types="j:j-up:j-q"/>				
				<Param event_types="q:q-up"/>				
				<Param event_types="ground:air"/>				
			</ComponentTmpl>
			<ComponentTmpl family="AI" type="SteerVehicle" priority="40"/>
			<ComponentTmpl family="Scene" type="Model" priority="100"/>
			<ComponentTmpl family="Audio" type="Sound3d" priority="50"/>
			<ComponentTmpl family="PhysicsControl" type="CharacterController" priority="90"/> 
		</ObjectTmpl>
		
		<ObjectTmpl type="StaticActor">
			<ComponentTmpl family="Scene" type="Model" priority="100"/>
			<ComponentTmpl family="Physics" type="RigidBody" priority="90"/> 
		</ObjectTmpl>
		
		<!-- templates for instancing -->
		<ObjectTmpl type="Instanceable">
			<ComponentTmpl family="Scene" type="Model" priority="100"/>
		</ObjectTmpl>
		
		<ObjectTmpl type="InstancedActor">
			<ComponentTmpl family="Scene" type="InstanceOf" priority="100"/>
			<ComponentTmpl family="Audio" type="Sound3d" priority="50"/>
			<ComponentTmpl family="Physics" type="RigidBody" priority="90"/> 
		</ObjectTmpl>
		
		<ObjectTmpl type="InstancedActorAI">
			<ComponentTmpl family="Scene" type="InstanceOf" priority="100"/>
			<ComponentTmpl family="AI" type="Steering" priority="40"/>
			<ComponentTmpl family="Audio" type="Sound3d" priority="50"/>
			<ComponentTmpl family="PhysicsControl" type="CharacterController" priority="90"/>
		</ObjectTmpl>

		<!-- templates for AI navmesh -->
		<ObjectTmpl type="RecastCrowdAgent">
			<ComponentTmpl family="Scene" type="InstanceOf" priority="100"/>
			<ComponentTmpl family="AI" type="CrowdAgent" priority="40"/>
			<ComponentTmpl family="Audio" type="Sound3d" priority="50"/>
			<ComponentTmpl family="Physics" type="RigidBody" priority="90"/>
		</ObjectTmpl>

		<ObjectTmpl type="RecastNavMesh">
			<ComponentTmpl family="Scene" type="Model" priority="100"/>
			<ComponentTmpl family="Physics" type="RigidBody" priority="90"/> 
			<ComponentTmpl family="AI" type="NavMesh" priority="80">
				<Param event_types="add_crowd_agent:remove_crowd_agent"/>
				<Param event_types="toggle_debug"/>
			</ComponentTmpl>
		</ObjectTmpl>

		<!-- templates for AI OpenSteer -->
		<ObjectTmpl type="OpenSteerVehicle">
			<ComponentTmpl family="Scene" type="InstanceOf" priority="100"/>
			<ComponentTmpl family="AI" type="SteerVehicle" priority="40"/>
			<ComponentTmpl family="Audio" type="Sound3d" priority="50"/>
			<ComponentTmpl family="Physics" type="RigidBody" priority="90"/>
		</ObjectTmpl>

		<ObjectTmpl type="OpenSteerPlugIn">
			<ComponentTmpl family="AI" type="SteerPlugIn" priority="80">
				<Param event_types="add_steer_vehicle:remove_steer_vehicle"/>
				<Param event_types="hit_obstacle:hit_vehicle"/>
				<Param event_types="toggle_debug"/>
			</ComponentTmpl>
		</ObjectTmpl>

		<!-- templates for vehicles -->
		<ObjectTmpl type="Car">
			<ComponentTmpl family="Scene" type="Model" priority="100"/>
			<ComponentTmpl family="Physics" type="RigidBody" priority="90"/>
			<ComponentTmpl family="PhysicsControl" type="Vehicle" priority="80">
				<Param event_types="forward:stop-forward"/>
				<Param event_types="backward:stop-backward"/>
				<Param event_types="turn_left:stop-turn_left"/>
				<Param event_types="turn_right:stop-turn_right"/>
				<Param event_types="brake:stop-brake"/>
			</ComponentTmpl>
		</ObjectTmpl>
		
		<ObjectTmpl type="CarWheel">
			<ComponentTmpl family="Scene" type="InstanceOf" priority="100"/>
		</ObjectTmpl>

		<!-- templates for soft bodies -->
		<ObjectTmpl type="SoftObject">
			<ComponentTmpl family="Scene" type="Model" priority="100"/>
			<ComponentTmpl family="Physics" type="SoftBody" priority="90"/> 
		</ObjectTmpl>
		
		<!-- templates for terrains -->
		<ObjectTmpl type="Terrain">
			<ComponentTmpl family="Scene" type="Terrain" priority="100"/>
			<ComponentTmpl family="Physics" type="RigidBody" priority="90"/> 
		</ObjectTmpl>

	</ObjectTmplSet>

